<h1>新規登録画面</h1>

<%= form_with(model: @user, local: true) do |f| %>

  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  
  <%= f.label :email, "E-mail" %>
  <%= f.text_field :email %>
  
  <%= f.label :password, "Password" %>
  <%= f.text_field :password %>
  
  <%= f.label :password_confirmation, "Confirmation" %>
  <%= f.text_field :password_confirmation %>
  
  <%= f.submit "登録" %>
  
<% end %>
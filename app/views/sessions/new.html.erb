<h1>ログイン画面</h1>

<%= form_with(url: login_path, scope: :session, local: true) do |f| %>

  <%= f.label :email, "E-mail" %>
  <%= f.text_field :email %>
  
  <%= f.label :password, "Password" %>
  <%= f.text_field :password %>
  
  <%= f.submit "ログイン" %>
  
<% end %>

<%= link_to "新規登録", signup_path %>